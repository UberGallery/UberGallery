{
    "name": "phlak/ubergallery",
    "description": "The simple PHP web photo gallery",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Kankiewicz",
            "email": "Chris@ChrisKankiewicz.com"
        }
    ],
    "support": {
        "issues": "https://github.com/UberGallery/UberGallery/issues"
    },
    "require": {
        "php": ">=7.4",
        "ext-imagick": "*",
        "filp/whoops": "^2.7",
        "php-di/php-di": "^6.2",
        "php-di/slim-bridge": "^3.0",
        "psr/http-message": "^1.0",
        "psy/psysh": "^0.10.4",
        "slim/psr7": "^1.1",
        "slim/slim": "^4.5",
        "slim/twig-view": "^3.1",
        "symfony/cache": "^5.1",
        "symfony/translation": "^5.1",
        "symfony/var-dumper": "^5.1",
        "symfony/yaml": "^5.1",
        "tightenco/collect": "^7.12",
        "vlucas/phpdotenv": "^5.0"
    },
    "require-dev": {
        "php": ">=7.4",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0",
        "symfony/css-selector": "^4.0",
        "symfony/dom-crawler": "^4.0",
        "vimeo/psalm": "^3.12"
    },
    "suggest": {
        "ext-apcu": "Required to use the APCu cache driver",
        "ext-memcached": "Required to use the Memcached driver",
        "ext-redis": "Required to use the Redis cache driver"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "serve": "php -S localhost:8080 -t public"
    }
}
